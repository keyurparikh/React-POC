{
  "name": "react-poc",
  "version": "1.0.0",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc --project .",
    "build-prod": "cd client && npm install && npm run build && cd .. && npm install && npm run build && npm run start",
    "server": "nodemon --verbose --watch \"server\" --ext \"ts,json\" --exec ts-node server/server.ts",
    "client": "cd client && npm start && cd ..",
    "dev": "concurrently \"npm run client\" \"npm run server\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keyurparikh/React-POC.git"
  },
  "eslintConfig": {},
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/keyurparikh/React-POC/issues"
  },
  "homepage": "https://github.com/keyurparikh/React-POC#readme",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.12",
    "concurrently": "^6.4.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.3"
  },
  "engines": {
    "node": "6.9.x"
  }
}
